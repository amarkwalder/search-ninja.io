{
    "workbench.iconTheme": "eq-material-theme-icons-palenight",
    "workbench.colorTheme": "Community Material Theme Palenight High Contrast",
    "workbench.tree.indent": 20,
    "window.zoomLevel": 1,
    "terminal.integrated.shell.osx": "/bin/zsh",
    "terminal.integrated.rendererType": "dom",
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "files.exclude": {
        "**/node_modules": true
    },
    "todo-tree.tree.showScanModeButton": false,
    "todo-tree.highlights.enabled": false,
    "todo-tree.filtering.useBuiltInExcludes": "file excludes",
    "browser-preview.chromeExecutable": "/Applications/Google Chrome.app/Contents/MacOS/Google Chrome",
    "browser-preview.startUrl": "http://localhost:3000",
    "liveSassCompile.settings.formats": [
        {
            "format": "compressed",
            "extensionName": ".min.css",
            "savePath": "/dist/css"
        }
    ],
    "liveSassCompile.settings.generateMap": false,
    "color-highlight.markerType": "dot-before",
    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#FF2D00",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "?",
            "color": "#3498DB",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": true,
            "backgroundColor": "transparent"
        },
        {
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "fixme",
            "color": "#FF8C00",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "*",
            "color": "#98C379",
            "strikethrough": false,
            "backgroundColor": "transparent"
        }
    ],
    "gitlens.defaultDateFormat": "",
    "gitlens.defaultDateShortFormat": "",
    "gitlens.blame.dateFormat": "",
    "gitlens.currentLine.dateFormat": "",
    "editor.minimap.enabled": false,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.detectIndentation": false,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.alwaysShowStatus": true,
    "eslint.format.enable": true,
    "eslint.packageManager": "yarn",
    "prettier.packageManager": "yarn",
    "yaml.customTags": [
        "!And",
        "!If",
        "!Not",
        "!Equals",
        "!Or",
        "!FindInMap sequence",
        "!Base64",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Join sequence"
    ],
    "yaml.format.enable": true,

    "files.associations": {
        "*.svelte": "html"
    }
}
